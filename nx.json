{
  "npmScope": "devon4ts-samples",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "angular-basic-pwa": {
      "tags": []
    },
    "angular-basic-pwa-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-basic-pwa"]
    },
    "angular-error-handler": {
      "tags": []
    },
    "angular-error-handler-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-error-handler"]
    },
    "angular-lazy-loading": {
      "tags": []
    },
    "angular-lazy-loading-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-lazy-loading"]
    },
    "angular-library": {
      "tags": []
    },
    "angular-library-test": {
      "tags": []
    },
    "angular-library-test-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-library-test"]
    },
    "angular-material-basic-layout": {
      "tags": []
    },
    "angular-material-basic-layout-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-material-basic-layout"]
    },
    "angular-material-theming": {
      "tags": []
    },
    "angular-material-theming-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-material-theming"]
    },
    "angular-mock-service": {
      "tags": []
    },
    "angular-mock-service-e2e": {
      "tags": [],
      "implicitDependencies": ["angular-mock-service"]
    },
    "ionic-basic-pwa": {

      "tags": []

    },
    "ionic-basic-pwa-e2e": {

      "tags": [],

      "implicitDependencies": ["ionic-basic-pwa"]

    },
    "shared-components": {
      "tags": []
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "angular-basic-pwa"
}
